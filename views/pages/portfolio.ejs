    <div class="works-about-div">
        <% works.forEach( work => { %>
            <%- include('../partials/work.ejs', { work: work }) %>
        <% }) %>
    </div>
    <div class="works-archive">
        <a href="https://github.com/codebygustavo?tab=repositories" target="_blank">Todos<sup>
                <%= allWorks %>
            </sup></a>
